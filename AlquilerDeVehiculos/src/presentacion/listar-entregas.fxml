<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="483.0" prefWidth="933.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.ControladorListarEntregas">
   <center>
      <TableView fx:id="entregas" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="fecha" maxWidth="1.7976931348623157E308" prefWidth="136.0" text="Fecha" />
            <TableColumn fx:id="tipoSeguro" prefWidth="147.0" text="Tipo de Seguro" />
            <TableColumn fx:id="kms" prefWidth="83.0" text="KMs" />
            <TableColumn fx:id="combustible" prefWidth="131.0" text="Combustible" />
            <TableColumn fx:id="cocheAsignado" minWidth="0.0" prefWidth="145.0" text="Coche Asignado" />
            <TableColumn fx:id="empleado" prefWidth="211.0" text="Empleado" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <Button fx:id="aceptar" mnemonicParsing="false" text="Aceptar" BorderPane.alignment="TOP_RIGHT">
         <BorderPane.margin>
            <Insets bottom="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
