<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.ControladorPrincipal">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Achivo">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#salir" text="Salir" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Cliente">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#crearReserva" text="Crear Reserva" />
                  <MenuItem mnemonicParsing="false" onAction="#crearCliente" text="Crear Cliente" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Empleado">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#crearCliente" text="Crear Cliente" />
                  <MenuItem mnemonicParsing="false" onAction="#crearSucursal" text="Crear Sucursal" />
                  <MenuItem mnemonicParsing="false" onAction="#crearCategoria" text="Crear Categoria" />
                  <MenuItem mnemonicParsing="false" onAction="#listarEntregas" text="Listar Entregas" />
                  <MenuItem mnemonicParsing="false" onAction="#listarReservas" text="Entregar Coche Reservado" />
                  <MenuItem mnemonicParsing="false" onAction="#listarReservasSucursal" text="Listar Reservas Sucursal" />
                  <MenuItem mnemonicParsing="false" onAction="#listarCoches" text="Listar Coches" />
                  <MenuItem mnemonicParsing="false" onAction="#listarCategorias" text="Listar Categorias" />
                  <MenuItem mnemonicParsing="false" onAction="#listarClientes" text="Listar Clientes" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Admin">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#listarSucursales" text="Listar sucursales" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Ayuda">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#AcercaDe" text="Acerca De" /> 
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <HBox prefHeight="300.0" prefWidth="500.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
